package Vista;

import javax.swing.JFrame;
import javax.swing.JPanel;
import java.awt.BorderLayout;
import java.awt.Dimension;
import javax.swing.JMenuBar;
import javax.swing.JMenu;
import javax.swing.JMenuItem;
import java.awt.GridBagLayout;

public class Principal {


	private JFrame jFrame = null;  //  @jve:decl-index=0:visual-constraint="125,19"
	private JPanel jContentPane = null;
	private JMenuBar jJMenuBar = null;
	private JMenu Opciones = null;
	private JMenuItem Calculadora = null;
	private JMenuItem Ecuaciones = null;
	private JPanel jPanel = null;

	public Principal(){
		getJFrame();
	}
	/**
	 * This method initializes jFrame	
	 * 	
	 * @return javax.swing.JFrame	
	 */
	private JFrame getJFrame() {
		if (jFrame == null) {
			jFrame = new JFrame();
			jFrame.setTitle("Principal");
			jFrame.setSize(new Dimension(500, 300));
			jFrame.setJMenuBar(getJJMenuBar());
			jFrame.setContentPane(getJContentPane());
		}
		return jFrame;
	}

	/**
	 * This method initializes jContentPane	
	 * 	
	 * @return javax.swing.JPanel	
	 */
	private JPanel getJContentPane() {
		if (jContentPane == null) {
			jContentPane = new JPanel();
			jContentPane.setLayout(new BorderLayout());
			jContentPane.add(getJPanel(), BorderLayout.NORTH);
		}
		return jContentPane;
	}

	/**
	 * This method initializes jJMenuBar	
	 * 	
	 * @return javax.swing.JMenuBar	
	 */
	private JMenuBar getJJMenuBar() {
		if (jJMenuBar == null) {
			jJMenuBar = new JMenuBar();
			jJMenuBar.add(getOpciones());
		}
		return jJMenuBar;
	}

	/**
	 * This method initializes Opciones	
	 * 	
	 * @return javax.swing.JMenu	
	 */
	private JMenu getOpciones() {
		if (Opciones == null) {
			Opciones = new JMenu();
			Opciones.setText("Opciones");
			Opciones.add(getCalculadora());
			Opciones.add(getEcuaciones());
		}
		return Opciones;
	}

	/**
	 * This method initializes Calculadora	
	 * 	
	 * @return javax.swing.JMenuItem	
	 */
	private JMenuItem getCalculadora() {
		if (Calculadora == null) {
			Calculadora = new JMenuItem();
		}
		return Calculadora;
	}

	/**
	 * This method initializes Ecuaciones	
	 * 	
	 * @return javax.swing.JMenuItem	
	 */
	private JMenuItem getEcuaciones() {
		if (Ecuaciones == null) {
			Ecuaciones = new JMenuItem();
		}
		return Ecuaciones;	}

	/**
	 * This method initializes jPanel	
	 * 	
	 * @return javax.swing.JPanel	
	 */
	private JPanel getJPanel() {
		if (jPanel == null) {
			jPanel = new JPanel();
			jPanel.setLayout(new GridBagLayout());
		}
		return jPanel;
	}

	public static void main(String[] args) {
		new Principal();

	}

}
